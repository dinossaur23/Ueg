<p id="notice"><%= notice %></p>

<h1>Uevs</h1>

<table>
  <thead>
    <tr>
      <th>Token</th>
      <th>Server</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @uevs.each do |uev| %>
      <tr>
        <td><%= uev.token %></td>
        <td><%= uev.uev_server %></td>
        <td><%= link_to 'Show', uev %></td>
        <td><%= link_to 'Edit', edit_uev_path(uev) %></td>
        <td><%= link_to 'Destroy', uev, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<div class="actions waves-effect waves-light btn-large" style="margin: 10px;">
  <a><%= link_to 'New Uev', new_uev_path, { style: 'color: white;' } %></a>
</div>
